{
    "type": "origins:multiple",
    "hidden": true,

    "color_change": {
        "type": "origins:model_color",
            "alpha": 0.8,
            "red": 0.35,
            "green": 0.5,
            "blue": 0.6
    },
    "speed": {
        "type": "origins:action_over_time",
        "interval": 5,
        "entity_action": {
            "type": "origins:execute_command",
            "command": "effect give @s speed 2 0 true"
        }
    },
    "weakness": {
        "type": "origins:action_over_time",
        "interval": 5,
        "entity_action": {
            "type": "origins:execute_command",
            "command": "effect give @s weakness 2 0 true"
        }
    },
    "ghost_tag": {
        "type": "origins:action_on_callback",
        "entity_action_gained": {
            "type": "origins:execute_command",
            "command": "tag @s add ghost",
            "permission_level": 4
        },
        "entity_action_lost": {
            "type": "origins:execute_command",
            "command": "tag @s remove ghost",
            "permission_level": 4
        }
    }
}
