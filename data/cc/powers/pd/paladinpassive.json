{
    "type": "origins:multiple",
    "hidden": true,

    "name": "Protector Presence",
    "description": "You have a halo above your head making you more resistant to damage",

    "paladin_tag": {
        "type": "origins:action_on_callback",
        "entity_action_gained": {
            "type": "origins:execute_command",
            "command": "tag @s add paladin",
            "permission_level": 4
        },
        "entity_action_lost": {
            "type": "origins:execute_command",
            "command": "tag @s remove paladin",
            "permission_level": 4
        }
    },
    "life_tag": {
        "type": "origins:action_on_callback",
        "entity_action_gained": {
            "type": "origins:execute_command",
            "command": "tag @s add life",
            "permission_level": 4
        },
        "entity_action_lost": {
            "type": "origins:execute_command",
            "command": "tag @s remove life",
            "permission_level": 4
        }
    },
    "paladin_halo_summon": {
        "type": "origins:action_over_time",
        "interval": 2,
        "entity_action": {
            "type": "origins:and",
            "actions": [
        {
            "type": "origins:execute_command",
            "command": "execute at @s positioned ~ ~2.45 ~ run function cc:halo/paladinhalo_particle",
            "permission_level": 4
        }
            ]
        }
    },

    "resistance": {
        "type": "origins:action_over_time",
        "interval": 5,
        "entity_action": {
            "type": "origins:execute_command",
            "command": "effect give @s resistance 2 0 true"
        }
    },
    "slowness": {
        "type": "origins:action_over_time",
        "interval": 5,
        "entity_action": {
            "type": "origins:execute_command",
            "command": "effect give @s slowness 2 0 true"
        }
    }
}
