    "mana_drain_overtime": {
        "type": "origins:action_over_time",
        "interval": 20,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:mr/mana",
            "change": -5
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "*:sn/sentinelshot_action"
                },
                {
                    "type": "origins:resource",
                    "resource": "*:mr/mana",
                    "comparison": ">",
                    "compare_to": 0
                },
                {
                    "type": "origins:resource",
                    "resource": "*:sn/scharge",
                    "comparison": "<",
                    "compare_to": 100
                }
            ]
        }
    },
